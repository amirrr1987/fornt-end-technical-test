#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/amaghami/Documents/GitHub/themovie/node_modules/.pnpm/nitropack@2.4.1/node_modules/nitropack/dist/node_modules:/mnt/c/Users/amaghami/Documents/GitHub/themovie/node_modules/.pnpm/nitropack@2.4.1/node_modules/nitropack/node_modules:/mnt/c/Users/amaghami/Documents/GitHub/themovie/node_modules/.pnpm/nitropack@2.4.1/node_modules:/mnt/c/Users/amaghami/Documents/GitHub/themovie/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/amaghami/Documents/GitHub/themovie/node_modules/.pnpm/nitropack@2.4.1/node_modules/nitropack/dist/node_modules:/mnt/c/Users/amaghami/Documents/GitHub/themovie/node_modules/.pnpm/nitropack@2.4.1/node_modules/nitropack/node_modules:/mnt/c/Users/amaghami/Documents/GitHub/themovie/node_modules/.pnpm/nitropack@2.4.1/node_modules:/mnt/c/Users/amaghami/Documents/GitHub/themovie/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../nitropack/dist/cli.mjs" "$@"
else
  exec node  "$basedir/../nitropack/dist/cli.mjs" "$@"
fi
