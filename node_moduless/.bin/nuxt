#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/amaghami/Documents/GitHub/themovie/node_modules/.pnpm/nuxt@3.5.2_@types+node@18.0.0/node_modules/nuxt/bin/node_modules:/mnt/c/Users/amaghami/Documents/GitHub/themovie/node_modules/.pnpm/nuxt@3.5.2_@types+node@18.0.0/node_modules/nuxt/node_modules:/mnt/c/Users/amaghami/Documents/GitHub/themovie/node_modules/.pnpm/nuxt@3.5.2_@types+node@18.0.0/node_modules:/mnt/c/Users/amaghami/Documents/GitHub/themovie/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/amaghami/Documents/GitHub/themovie/node_modules/.pnpm/nuxt@3.5.2_@types+node@18.0.0/node_modules/nuxt/bin/node_modules:/mnt/c/Users/amaghami/Documents/GitHub/themovie/node_modules/.pnpm/nuxt@3.5.2_@types+node@18.0.0/node_modules/nuxt/node_modules:/mnt/c/Users/amaghami/Documents/GitHub/themovie/node_modules/.pnpm/nuxt@3.5.2_@types+node@18.0.0/node_modules:/mnt/c/Users/amaghami/Documents/GitHub/themovie/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../nuxt/bin/nuxt.mjs" "$@"
else
  exec node  "$basedir/../nuxt/bin/nuxt.mjs" "$@"
fi
